  "scripts": {
    "start:web": "nx run web-client:dev",
    "start:api": "nx run web-api:serve",
    "serve:web": "cd apps/web-client && next start -p 3000",
    "serve:api": "cd apps/web-api && node dist/main.js",
    "start": "nx run-many --targets=serve,dev --projects=web-api,web-client --parallel",
    "dev": "nx run-many --targets=serve,dev --projects=web-api,web-client --parallel",
    "build:web": "nx run web-client:build --exclude-task-dependencies",
    "build:api": "nx run web-api:build",
    "test:web": "nx run web-client:test",
    "lint": "nx run-many --target=lint --projects=web-client,web-api,ui --fix"
  },
